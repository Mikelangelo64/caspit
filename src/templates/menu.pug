    },
    {
      name: '+357 220 529 18',
      href: 'tel:+35722052918',
      goto: 'false'
    }
  ];

nav.menu
  ul.menu-list
    each item in menuList
      li.menu-list__item
        a.menu__link(
          class=`${item.goto !== 'false' ? 'anchor' : ''}`,
          href=`${item.href}`,
          data-goto=`${item.goto}`
        ) #{ item.name }

  .dropdown.mobile
    p.dropdown__header Наші проекти

    ul.dropdown-list
      each item in dropdownList
        li.dropdown-list__item
          a.dropdown-list__link(
            href=`${item.href}`,
            target='_blank',
            rel='noreferrer'
          )= item.name

  a.menu__link.menu__address(href='#') 17 Ifigeneias Street 2007 Strovolos, Nicosia Cyprus
